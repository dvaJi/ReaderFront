{
  "name": "ravens-scans-frontend",
  "version": "3.4.1",
  "homepage": "https://github.com/dvaJi/ReaderFront/",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lang": "babel scripts/translate.js | node",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --env=jsdom --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "release": "standard-version",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "^0.0.19",
    "bootstrap": "^4.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "history": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.4.1",
    "react-meta-tags": "^0.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.1",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-i18n": "^1.5.13",
    "redux-thunk": "^2.2.0",
    "sinon": "^5.0.7",
    "standard-version": "^4.3.0",
    "styled-components": "^3.2.6",
    "w3c-xmlhttprequest": "^2.1.2"
  },
  "devDependencies": {
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "~2.13.2",
    "source-map-explorer": "^1.5.0"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost/FoOlSlide",
      "ws": true
    },
    "/content": {
      "target": "http://localhost/FoOlSlide",
      "ws": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/Routes.js",
      "!src/registerServiceWorker.js",
      "!src/common/WithTracker.js",
      "!src/App.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
