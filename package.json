{
  "name": "readerfront",
  "version": "4.3.0",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "manga-cms",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node --icu-data-dir=node_modules/full-icu server.js",
    "dev": "node --icu-data-dir=node_modules/full-icu server.js",
    "dev:admin": "cd ./admin && npm run start",
    "build": "next build",
    "build:admin": "cd ./admin && npm run build",
    "test": "jest --watchAll --verbose",
    "test:ci": "jest --coverage",
    "test:admin": "cd ./admin && npm run test:ci",
    "setup:db": "cd ./api && npm run setup:db",
    "db:migrate": "cd ./api && npm run db:migrate",
    "db:seed": "cd ./api && npm run db:seed",
    "db:drop": "cd ./api && npm run db:drop",
    "release": "standard-version",
    "lint": "eslint src",
    "prettier": "pretty-quick --staged"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@formatjs/intl-relativetimeformat": "^4.5.10",
    "@formatjs/intl-utils": "^2.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "accepts": "^1.3.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.11.1",
    "dotenv": "^8.2.0",
    "full-icu": "^1.3.1",
    "glob": "^7.1.6",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "next": "^9.3.3",
    "next-cookies": "^2.0.3",
    "next-offline": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-hooks-global-state": "^1.0.0",
    "react-intl": "^3.12.1",
    "react-lazy-load-image-component": "^1.4.1",
    "react-lazyload": "^2.6.6",
    "react-markdown": "^4.3.1",
    "react-spring": "^8.0.27",
    "reactstrap": "^8.4.1",
    "standard-version": "^7.1.0",
    "styled-components": "^5.0.1",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/runtime-corejs2": "^7.9.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run prettier && npm run lint"
    }
  }
}
