{
  "name": "readerfront",
  "version": "4.3.0",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "manga-cms",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node --icu-data-dir=node_modules/full-icu server.js",
    "dev": "node --icu-data-dir=node_modules/full-icu server.js",
    "dev:admin": "cd ./admin && npm run start",
    "dev:api": "cd ./api && npm run start",
    "dev:all": "concurrently \"npm:dev\" \"npm:dev:api\" \"npm:dev:admin\"",
    "build": "next build",
    "build:admin": "cd ./admin && npm run build",
    "build:api": "cd ./api && npm run build",
    "build:all": "npm run build && npm run build:api && npm run build:admin",
    "test": "jest --watchAll --verbose",
    "test:ci": "jest --config ./jest.config.js --coverage",
    "test:admin": "cd ./admin && npm run test:ci",
    "test:api": "cd ./api && npm run test:ci",
    "setup:db": "cd ./api && npm run setup:db",
    "db:migrate": "cd ./api && npm run db:migrate",
    "db:seed": "cd ./api && npm run db:seed",
    "db:drop": "cd ./api && npm run db:drop",
    "release": "standard-version",
    "lint": "eslint src",
    "lint:admin": "cd ./admin && npm run lint",
    "lint:api": "cd ./api && npm run lint",
    "prettier": "pretty-quick --staged",
    "analyze": "cross-env ANALYZE=true NODE_ENV=production npm run build"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@formatjs/intl-relativetimeformat": "^2.8.2",
    "@formatjs/intl-utils": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "accepts": "^1.3.7",
    "bootstrap": "^4.6.0",
    "concurrently": "^6.0.0",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "full-icu": "^1.3.1",
    "glob": "^7.1.6",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.8.4",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^2.2.1",
    "next": "^10.0.8",
    "next-cookies": "^2.0.3",
    "next-offline": "^5.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-hooks-global-state": "^1.0.1",
    "react-intl": "^5.15.7",
    "react-lazy-load-image-component": "^1.5.1",
    "react-lazyload": "^2.6.9",
    "react-markdown": "^5.0.3",
    "react-spring": "^8.0.27",
    "reactstrap": "^8.9.0",
    "standard-version": "^9.1.1",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.13.10",
    "@commitlint/config-conventional": "^12.0.1",
    "@next/bundle-analyzer": "^10.0.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-graphql-tag": "^3.2.0",
    "babel-plugin-styled-components": "^1.12.0",
    "commitlint": "^12.0.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^4.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run prettier && npm run lint && npm run lint:admin && npm run lint:api"
    }
  }
}
