{
  "name": "readerfront",
  "version": "4.3.0",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "lint": "eslint src",
    "prettier": "pretty-quick --staged",
    "analyze": "yarn build && source-map-explorer build/static/js/*.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "release": "standard-version",
    "manage:translations": "node ./translationRunner.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-boost": "^0.3.1",
    "apollo-link-context": "^1.0.18",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.4.0",
    "date-fns": "^1.30.1",
    "graphql": "^14.4.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "http-proxy-middleware": "^0.19.1",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-datepicker": "^2.7.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-ga": "^2.6.0",
    "react-helmet": "^5.2.1",
    "react-hooks-global-state": "^0.9.0",
    "react-intl": "^2.9.0",
    "react-intl-redux": "^2.1.1",
    "react-lazyload": "^2.6.1",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.1.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-rte": "^0.16.1",
    "react-scripts": "^3.0.1",
    "react-spring": "^8.0.25",
    "react-test-renderer": "^16.8.6",
    "react-transition-group": "^4.1.1",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@anilanar/moxios": "^0.9.0",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/react-intl": "^2.3.18",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-react-intl": "^2.0.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.7.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-styled-components": "^6.3.3",
    "jsdom-global": "^3.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-intl-translations-manager": "^5.0.3",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "source-map-explorer": "^1.8.0",
    "standard-version": "^6.0.1",
    "wait-for-expect": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn prettier && yarn lint"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/Routes.js",
      "!src/registerServiceWorker.js",
      "!src/common/WithTracker.js",
      "!src/App.js",
      "!src/utils/mocks/*.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
