{
  "name": "readerfront-admin",
  "version": "4.3.0",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "manga-cms",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --no-cache",
    "test:ci": "react-app-rewired test --coverage",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-upload-client": "^13.0.0",
    "bootstrap": "^4.5.0",
    "downshift": "^5.4.2",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "history": "^4.10.1",
    "http-proxy-middleware": "^1.0.4",
    "js-cookie": "^2.2.1",
    "react": "^16.13.1",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-ga": "^3.0.0",
    "react-helmet": "^6.0.0",
    "react-hooks-global-state": "^1.0.0",
    "react-intl": "^4.6.9",
    "react-lazy-load-image-component": "^1.4.3",
    "react-lazyload": "^2.6.7",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-rte": "^0.16.1",
    "react-scripts": "^3.4.1",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.13.1",
    "react-transition-group": "^4.4.1",
    "reactstrap": "^8.4.1",
    "styled-components": "^5.1.1",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@types/react-intl": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-standard": "^4.0.1",
    "jest-localstorage-mock": "^2.4.2",
    "jest-styled-components": "^7.0.2",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6",
    "symlink-webpack-plugin": "^1.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@components(.*)$": "<rootDir>/src/components$1"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/Routes.js",
      "!<rootDir>/src/registerServiceWorker.js",
      "!<rootDir>/src/common/WithTracker.js",
      "!<rootDir>/src/(state|themes|config|setupApollo|setupProxy|setupIcons|Root|App).js",
      "!<rootDir>/src/auth/(AuthCheck|RoutePrivate).js",
      "!<rootDir>/src/utils/mocks/*.{js}",
      "!<rootDir>/src/utils/(helpers|common).js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
