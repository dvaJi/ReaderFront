{
  "name": "readerfront",
  "version": "4.1.0",
  "homepage": "https://github.com/dvaJi/ReaderFront/",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "lint": "eslint src",
    "prettier": "pretty-quick --staged",
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "release": "standard-version",
    "manage:translations": "node ./translationRunner.js"
  },
  "dependencies": {
    "@dvaji/react-responsive": "^0.0.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@opuscapita/react-markdown": "^2.3.9",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "history": "^4.7.2",
    "http-proxy-middleware": "^0.19.0",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.6.3",
    "react-dropzone": "8.0.1",
    "react-ga": "^2.5.6",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "react-intl-redux": "^2.0.2",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.0.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "^2.1.1",
    "react-test-renderer": "^16.6.3",
    "react-transition-group": "^2.5.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "simple-slugify-string": "^1.0.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@anilanar/moxios": "^0.9.0",
    "@types/react-intl": "^2.3.12",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-react-intl": "^2.0.3",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.4",
    "jsdom-global": "^3.0.2",
    "pretty-quick": "^1.8.0",
    "react-intl-translations-manager": "^5.0.3",
    "redux-devtools": "^3.4.2",
    "redux-devtools-extension": "~2.13.7",
    "redux-mock-store": "^1.5.3",
    "source-map-explorer": "^1.6.0",
    "standard-version": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn prettier"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/Routes.js",
      "!src/registerServiceWorker.js",
      "!src/common/WithTracker.js",
      "!src/App.js",
      "!src/utils/mocks/*.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
