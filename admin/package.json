{
  "name": "readerfront-admin",
  "version": "4.3.0",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "manga-cms",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-upload-client": "^12.1.0",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.9.0",
    "downshift": "^4.0.7",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "http-proxy-middleware": "^0.20.0",
    "js-cookie": "^2.2.1",
    "react": "^16.12.0",
    "react-datepicker": "^2.11.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-hooks-global-state": "^0.17.0",
    "react-intl": "^3.11.0",
    "react-lazy-load-image-component": "^1.4.1",
    "react-lazyload": "^2.6.5",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-rte": "^0.16.1",
    "react-scripts": "^3.3.0",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "reactstrap": "^8.4.0",
    "styled-components": "^5.0.0",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@types/react-intl": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "jest-styled-components": "^7.0.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^1.19.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!./src/index.js",
      "!./src/Routes.js",
      "!./src/registerServiceWorker.js",
      "!./src/common/WithTracker.js",
      "!<rootDir>/src/(state|themes|config|setupApollo|setupProxy|setupIcons|Root|App).js",
      "!./src/auth/(AuthCheck|RoutePrivate).js",
      "!./src/utils/mocks/*.{js}",
      "!./src/utils/(helpers|common).js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
